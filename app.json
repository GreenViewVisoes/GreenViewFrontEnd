{
  "expo": {
    "name": "GreenViewMobile",
    "slug": "GreenViewMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E5E5E5"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E5E5E5"
      },
      "package": "com.greenview.greenview"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font", "expo-image-picker", "expo-camera"],
    "extra": {
      "eas": {
        "projectId": "6c75313f-84c6-450e-a6a5-fa137b6ad47d"
      }
    }
  }
}
